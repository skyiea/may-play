{
  "name": "jsx-suit",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prepare": "npm install -g webpack webpack-dev-server && npm install",
    "server": "nodemon --watch server.js --exec babel-node -- server.js",
    "build": "NODE_ENV=production webpack -p --bail",
    "watch": "webpack --progress --watch",
    "watchs": "SOURCE_MAPS=on webpack --progress --watch",
    "hot": "webpack-dev-server --inline --hot --progress",
    "hots": "SOURCE_MAPS=on webpack-dev-server --inline --hot --progress"
  },
  "dependencies": {
    "express": "^4.10.2",
    "lodash": "^3.10.1",
    "nodemon": "^1.8.1",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "webpack": "^1.12.6"
  },
  "devDependencies": {
    "autoimport-loader": "^0.2.4",
    "babel": "^5.8.23",
    "babel-core": "^5.8.34",
    "babel-eslint": "^4.1.5",
    "babel-loader": "^5.3.2",
    "css-loader": "^0.15.6",
    "eslint": "^1.9.0",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-react": "^3.9.0",
    "file-loader": "^0.8.5",
    "loader-utils": "^0.2.11",
    "node-sass": "^3.4.2",
    "react-core-decorators": "0.0.1",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.12.4",
    "url-loader": "^0.5.7",
    "webpack-dev-server": "^1.12.1"
  }
}
